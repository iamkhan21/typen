{
  "name": "typen",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "update:deps": "pnpm up -r -i -L && pnpm i"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "2.7.1",
    "turbo": "1.4.3"
  },
  "engines": {
    "pnpm": ">=7.0.0",
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.9.3"
}
